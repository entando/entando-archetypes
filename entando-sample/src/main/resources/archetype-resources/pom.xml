    <profiles>
        <profile>
            <id>production</id>
            <activation>
                <property>
                    <name>env.MAVEN_WORKING_MODE</name>
                    <value>production</value>
                </property>
            </activation>
            <properties>
                <env>production</env>
                <env.db.environment>production</env.db.environment>
            </properties>
        </profile>
        <profile>
            <id>development-unix</id>
            <activation>
                <activeByDefault>true</activeByDefault>
                <property>
                    <name>env.MAVEN_WORKING_MODE</name>
                    <value>development-unix</value>
                </property>
            </activation>
            <properties>
                <env>development-unix</env>
                <env.db.environment>develop</env.db.environment>
            </properties>
        </profile>
        <profile>
            <id>ci</id>
            <properties>
                <env>ci</env>
            </properties>
        </profile>
        <profile>
            <id>standalone</id>
            <activation>
                <property>
                    <name>env.MAVEN_WORKING_MODE</name>
                    <value>standalone</value>
                </property>
            </activation>
            <properties>
                <env>standalone</env>
                <env.db.environment>production</env.db.environment>
            </properties>
        </profile>
        <profile>
            <id>development-windows</id>
            <activation>
                <os>
                    <family>windows</family>
                </os>
            </activation>
            <properties>
                <env>development-windows</env>
                <env.db.environment>develop</env.db.environment>
            </properties>
        </profile>

        <!-- Added for jboss -->
        <profile>
            <id>production-jboss</id>
            <activation>
                <property>
                    <name>env.MAVEN_WORKING_MODE</name>
                    <value>production-jboss</value>
                </property>
            </activation>
            <properties>
                <env>production-jboss</env>
                <env.db.environment>production</env.db.environment>
                <jboss>jboss</jboss>
            </properties>
        </profile>

        <!-- Added for OpenShift -->
        <profile>
            <id>openshift</id>
            <activation>
                <property>
                    <name>env.MAVEN_WORKING_MODE</name>
                    <value>openshift</value>
                </property>
            </activation>
            <properties>
                <env>openshift</env>
                <env.db.environment>production</env.db.environment>
                <jboss>jboss</jboss>
            </properties>
        </profile>

        <profile>
            <id>sonarCheck</id>
            <build>
                <pluginManagement>
                    <plugins>
                        <!-- owasp plugin -->
                        <plugin>
                            <groupId>org.owasp</groupId>
                            <artifactId>dependency-check-maven</artifactId>
                            <version>5.0.0</version>
                            <executions>
                                <execution>
                                    <goals>
                                        <goal>aggregate</goal>
                                    </goals>
                                </execution>
                            </executions>
                            <configuration>
                                <autoUpdate>true</autoUpdate>
                                <formats>ALL</formats>
                                <skipProvidedScope>true</skipProvidedScope>
                            </configuration>
                        </plugin>
                        <!-- owasp plugin end -->
                    </plugins>
                </pluginManagement>
            </build>
        </profile>

    </profiles>
